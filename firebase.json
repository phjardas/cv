{
  "hosting": {
    "public": "public",
    "headers": [
      {
        "source": "*.@(js|css)",
        "headers": [{ "key": "Cache-Control", "value": "max-age=31536000" }]
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/en",
        "type": 301
      }
    ]
  }
}
