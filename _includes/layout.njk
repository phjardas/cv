      <div class="content">
        <main class="main">
          <h2>{{ 'employment-history' | i18n }}</h2>
          <div>
            {% for work in works %}
              <div>
                <h3>
                  {{ work.role }}
                  <small>{{ 'at' | i18n }}</small>
                  {{ work.company }}
                </h3>
                <p>
                  <small>
                    {{ work.from | date('MMMM yyyy') }}
                    –
                    {% if work.until %}
                      {{ work.until | date('MMMM yyyy') }}
                    {% else %}
                      {{ 'now' | i18n }}
                    {% endif %}
                    in {{ work.location }}
                  </small>
                </p>
                {% for line in work.description[language] %}
                  <p>{{ line }}</p>
                {% endfor %}
                {% if work.technologies %}
                  <p>
                    <small>
                      {% for tech in work.technologies %}
                        {{ tech }}
                        {%- if not loop.last %}, {% endif %}
                      {% endfor %}
                    </small>
                  </p>
                {% endif %}
              </div>
            {% endfor %}
          </div>
        </main>

        {{ content | safe }}
      </div>

      <footer class="page-footer">
        <div class="contact">
          {{ 'contact' | i18n }}:
          <a href="mailto:philipp@jardas.de">
            <span class="fa fa-envelope"></span>
            philipp@jardas.de
          </a>
          <a href="http://de.linkedin.com/in/philippjardas" class="profiles">
            <span class="fab fa-linkedin"></span>
            LinkedIn
          </a>
          <a href="https://github.com/phjardas" class="profiles">
            <span class="fab fa-github"></span>
            GitHub
          </a>
        </div>
